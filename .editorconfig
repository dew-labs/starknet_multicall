# editorconfig.org
# prettier config will reference this file, unless override is specified
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 100
tab_width = 4
indent_style = space

[*.yml]
indent_style = space
indent_size = 2
